{
  "name": "mockserver-client-builder",
  "version": "1.1.3",
  "description": "Powerful mockserver client wrapper based on builders with expectation, request matcher and action builders in TypeScript notation",
  "keywords": [
    "mockserver",
    "mockserver-client",
    "mock-restful-api",
    "expectation-builder",
    "mockserver-request-builder",
    "mockserver-response-builder",
    "mockserver-forward-builder",
    "mockserver-data-builder"
  ],
  "repository": "sem-soft/mockserver-client-builder",
  "author": "Vladimir Samsonov <hello@sem-soft.ru>",
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "types": "lib",
  "files": ["lib"],
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rf ./lib",
    "lint": "eslint . --ext .ts",
    "lint-n-fix": "eslint . --ext .ts --fix",
    "test": "jest --maxWorkers=1",
    "release": "npm run clean && npm run build && npm publish"
  },
  "dependencies": {
    "mockserver-client": "^5"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-jest": "^27.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^27.3.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  }
}
